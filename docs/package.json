{
  "name": "yamada-docs",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "clean": "rimraf .mdx .next .eslintcache *.tsbuildinfo node_modules",
    "dev": "tsx server",
    "start": "next start",
    "prebuild": "pnpm gen:content",
    "build": "next build",
    "build:profile": "next build --profile",
    "gen:tokens": "yamada-cli tokens theme/index.ts",
    "gen:content": "tsx scripts/mdx/builder",
    "gen:changelog": "tsx scripts/content/changelog",
    "gen:theme": "tsx scripts/component/theme",
    "gen:styled-system": "tsx scripts/content/styled-system",
    "gen:props": "tsx scripts/component/props",
    "gen:search": "tsx scripts/content/search",
    "gen:icons": "tsx scripts/generate-icon-tags && eslint pages/icons/tags.ts --fix",
    "gen:contributors": "tsx scripts/content/contributors && eslint constant/maintainers.ts constant/members.ts constant/emeriti.ts --fix",
    "gen:translate": "tsx scripts/content/translate",
    "gen:label": "tsx scripts/content/label",
    "typecheck": "tsc --noEmit",
    "format": "prettier --check --cache .",
    "lint": "eslint . --max-warnings=0 --cache",
    "test": "vitest",
    "test:dev": "vitest --watch --ui"
  },
  "dependencies": {
    "@emotion/cache": "11.11.0",
    "@emotion/core": "11.0.0",
    "@emotion/react": "11.11.0",
    "@emotion/weak-memoize": "^0.4.0",
    "@faker-js/faker": "^9.7.0",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@lucide/lab": "^0.1.2",
    "@tanstack/react-query": "^5.75.7",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "@yamada-ui/react": "workspace:*",
    "dayjs": "^1.11.13",
    "lucide-react": "^0.509.0",
    "match-sorter": "^8.0.1",
    "next": "^15.3.2",
    "next-mdx-remote": "^5.0.0",
    "next-seo": "^6.6.0",
    "prism-react-renderer": "^2.4.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.56.3",
    "react-icons": "^5.5.0",
    "react-live": "^4.1.8",
    "scroll-into-view-if-needed": "^3.1.0"
  },
  "devDependencies": {
    "@cspell/eslint-plugin": "^9.0.1",
    "@eslint/compat": "^1.2.9",
    "@next/eslint-plugin-next": "^15.3.2",
    "@octokit/request-error": "^6.1.8",
    "@octokit/rest": "^21.1.1",
    "@octokit/webhooks": "^13.8.2",
    "@types/jest": "^29.5.14",
    "@types/mdx": "^2.0.13",
    "@types/node": "^22.15.17",
    "@types/react": "^19.1.3",
    "@types/react-dom": "^19.1.3",
    "@typescript-eslint/parser": "^8.32.0",
    "@yamada-ui/cli": "workspace:*",
    "chokidar": "^4.0.3",
    "dotenv": "^16.5.0",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^4.3.4",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-import-replace": "^1.0.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-perfectionist": "^4.12.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-testing-library": "^7.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "execa": "9.3.1",
    "find-packages": "^10.0.4",
    "github-slugger": "^2.0.0",
    "glob": "^11.0.2",
    "gray-matter": "^4.0.3",
    "jiti": "^2.4.2",
    "markdown-toc": "^1.2.0",
    "markdown-wasm": "^1.2.0",
    "node-mocks-http": "^1.17.2",
    "openai": "^4.98.0",
    "prettier": "^3.5.3",
    "rehype-slug": "^6.0.0",
    "remark-breaks": "^4.0.0",
    "remark-emoji": "^5.0.1",
    "remark-gfm": "^4.0.1",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.0",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0",
    "vitest": "^3.1.3"
  }
}
